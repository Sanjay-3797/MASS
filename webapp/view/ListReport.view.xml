<mvc:View
    xmlns="sap.m"
    xmlns:mvc="sap.ui.core.mvc"
    controllerName="com.worksheet.worksheet.controller.ListReport"
    xmlns:l='sap.ui.layout'
    xmlns:f='sap.ui.layout.form'
    xmlns:core='sap.ui.core'
    height="100%"
>
    <App id="app">
        <Page
            id="page"
            showHeader="false"
        >
            <content>
                <core:HTML
                    id="iframeContainer"
                    content="{
                        path: 'BatchModel>/parent_reqId',
                        formatter: '.formatIframeSrc'
                    }"
                />
            </content>
        </Page>
    </App>
</mvc:View>
